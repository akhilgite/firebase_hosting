<html>
  <head>
    <title>Home Page</title>
  </head>

  <body>
    <h1>Welcome Home</h1>


    <form action="/submit">
        <div>
          <input name="link" type="text" placeholder="Enter Link">
        </div>
        <div> 
          <button id="submit">Submit</button>
        </div>
      </form>  

    <h1>Youtube Links</h1>
    <ul>
        {{#each links}}
        <li>{{text}}</li>
        {{/each}}
    </ul>
  </body>
</html>